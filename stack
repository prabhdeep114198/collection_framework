import java.util.Stack;
public class Demo_Stack {
    public static void main(String[] args) {
        Stack<String> animal = new Stack<>();
        animal.push("horse");
        animal.push("dog");
        animal.push("cat");
        animal.push("rat");
        System.out.println(animal);

        System.out.println(animal.peek());

        animal.pop();
        animal.pop();
        System.out.println(animal);


    }
}
